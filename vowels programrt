#include<stdio.h>
int main()
{
	int c=0,count=0;
	char s[1000];
	
	printf("input a string:");
	gets(s);
	
	while (s[c] !='\0'){
		if (s[c]=='a' || s[c]=='A' || s[c]=='E' || s[c]== 'e' || s[c]=='I'|| s[c]=='i' || s[c]=='o' || s[c]== 'O' || s[c]=='U' ||s[c]=='u')
			count++;
			c++;
		}
		printf("number of vowels in the string:%d",count);
	
	return 0;
}
