#include <stdio.h>

int main() {

  int i,j,size = 5;

  
  for ( i = 1; i <= size; i++) {
    for ( j = 0; j < i; j++) {
     
      if (i != size) {
       
        if (j == 0 || j == i-1) {
          printf("*");
        } else {
          printf(" ");
        }
      }
      
      else {
        printf("*");
      }
    }
    printf("\n");
  }
   return 0;
}
