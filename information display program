#include<stdio.h>
struct student
{
    char name[50];
    char mentordept[50];
    char address[50];
    long long int roll, phone;
}s;
int main()
{
 
    
    printf("Enter The Information of Students :\n\n");
        printf("Enter name :\n ");
    scanf("%s",&s.name);
    
    printf("Enter roll no. : \n");
    scanf("%lli",&s.roll);
    
    printf("enter mentor department :\n");
    scanf("%s",&s.mentordept);
    
    printf("enter address:\n");
    scanf("%s",&s.address);
    
    printf("enter phone number:\n");
    scanf("%lli",&s.phone);
    
    printf("\nDisplaying Information\n");
    
    printf("name: %s\n",s.name);
    printf("roll no: %lli\n",s.roll);
    printf("mentor department: %s\n",s.mentordept);
    printf("address: %s\n",s.address);
    printf("phone number %lli\n",s.phone);
    return 0;
}
